{
    "author doc": {
        "prefix": "adoc",
        "body": [
            "\"\"\" @author    : ZEISS IQSX",
            "\t@contact   : everett.mthunzi[@]zeiss.com",
            "\t@license   : Proprietary License",
            "\t@date      : ${1:02 Jun. 2022}",
            "\t@version   : ${2:0.2} Beta",
            "\t@status    : Production",
            "\"\"\""
        ],
        "description": "author doc"
    },
    "class doc": {
        "prefix": "cdoc",
        "body": [
            "\"\"\"",
            "\t@brief",
            "\t  ${1:a brief description}",
            "\t",
            "\t@rationale",
            "\t  ${2:a detailed description}",
            "\t",
            "\t@${3:members}",
            "\t  ${4:a} (${6:type}): ${8:description} ",
            "\t  ${5:b} (${7:type}): ${9:description} ",
            "\t",
            "\t@${10:methods}",
            "\t  (${11:ret type}) ${12:member name}: (${13:type}, ${14:type}, ${15:type}) ",
            "\"\"\""
        ],
        "description": "class doc"
    },
    "method doc": {
        "prefix": "mdoc",
        "body": [
            "\"\"\"",
            "\t@brief",
            "\t  ${1:a brief description}",
            "\t",
            "\t@rationale",
            "\t  ${2:a detailed description}",
            "\t",
            "\t@${3:params}",
            "\t ${4:a} (${6:type}): ${8:description} ",
            "\t ${5:b} (${7:type}): ${9:description} ",
            "\t",
            "\t@${10:rtype}",
            "\t  (${11:type}) ${12:description} ",
            "\t",
            "\t@${13:notes}",
            "\t  (${14:type}) ${15:description} ",
            "\"\"\""
        ],
        "description": "method doc"
    },
    "todo": {
        "prefix": "todo",
        "body": [
            "#",
            "# todo: ${1:todo}",
            "# date: ${2:date}",
            "#"
        ],
        "description": "todo note"
    }
}
